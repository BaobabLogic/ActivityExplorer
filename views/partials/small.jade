div(id="main", ng-style="mainStyle", ng-click="loadMore()", when-scrolled="loadMore()")
	div(ng-repeat="result in results", class="smallResult")
		div(class="smallImage") 
			image(ng-src='{{result.image[0]}}')
		div.details
			div 
				| Type
			div 
				| Location
				span(class="price") 
					| R{{price(result.price[0])}} 
			